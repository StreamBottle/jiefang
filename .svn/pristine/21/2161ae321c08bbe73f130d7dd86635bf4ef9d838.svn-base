<!-- 点击出现弹框 -->
<!-- [twinkle]="'twinkle'" 闪烁指令 -->
<div class="type_thirtyeight" *ngIf="appboolen.type_thirtyeight">
  <img class="f-mc g-wA80 f-pt60" src="{{typedescribe}}" />
  <ul>
    <ng-template ngFor let-i='index' let-item [ngForOf]='typetextlist'>
      <li (click)='tipShow(item,i,true)' [twinkle]="item.twinkle" [ngStyle]='item.style'></li>
      <div *ngIf='item.isShow' [ngStyle]='item.content.style'>{{item.content.text}}</div>
    </ng-template>
  </ul>
</div>
<!-- 课程目标等静态页面 -->
<div class="type-text" *ngIf='appboolen.type_text'>
  <img [ngStyle]='typeimglist[0].imgStyle' src="{{typeimglist[0].img}}" alt="">
  <ng-template ngFor let-item [ngForOf]='this.typetextlist'>
    <div [ngStyle]='item.titleStyle'>{{item.title}}</div>
  </ng-template>
</div>
<!-- 现场管理等静态页面 -->
<div class="type-icontext" *ngIf='appboolen.type_icontext'>
  <img [ngStyle]='typeimglist[0].imgStyle' src="{{typeimglist[0].img}}" alt="">
  <ul class="icontextlist">
    <ng-template ngFor let-item [ngForOf]='this.typetextlist'>
      <li>
        <div *ngIf='item.isShow' [ngStyle]='item.oneDivStyle' class="one"></div>
        <div [ngStyle]='item.style' class="two">{{item.text}}</div>
      </li>
      <!-- <img *ngIf='btnitem.imgShow' [ngStyle]='btnitem.srcStyle' src="{{btnitem.src}}" alt=""> -->
    </ng-template>
  </ul>
</div>
<!-- 课程目标等静态页面 -->
<div class="type-changeText" *ngIf='appboolen.type_changeText'>
  <img [ngStyle]='typeimglist[0].imgStyle' src="{{typeimglist[0].img}}" alt="">
  <ng-template ngFor let-item [ngForOf]='this.typetitlelist'>
    <div [ngStyle]='item.titleStyle'>{{item.title}}</div>
  </ng-template>
  <ng-template ngFor let-i='index' let-btnitem [ngForOf]='this.typebtnlist'>
    <div [ngStyle]='btnitem.style' (click)='tipContentShow($event,typebtnlist,i)'>{{btnitem.text}}</div>
    <ul *ngIf='btnitem.isShow'>
      <img [ngStyle]='btnitem.arrowStyle' src="./assets/images/project/arrow1.png" alt="">
      <ng-template ngFor let-btnContentItem [ngForOf]='btnitem.content'>
        <li [ngStyle]='btnContentItem.style'>{{btnContentItem.text}}</li>
      </ng-template>
    </ul>
  </ng-template>
</div>

<!-- 岗位描述等静态页面 -->
<div class="type_imgText" *ngIf='appboolen.type_imgText'>
  <div class="nav">
    <ng-template ngFor let-i='index' let-item [ngForOf]='typetextlist[0].navText'>
      <span (click)='rotateNav(item,i)' [ngStyle]='item.style'>{{item.text}}</span>
    </ng-template>
  </div>
  <ng-template ngFor let-='index' let-item [ngForOf]='typetextlist[0].content'>
    <div *ngIf='item.isShow'>
      <div [ngStyle]='item.style'>
        <ng-template ngFor let-i='index' let-item [ngForOf]='item.contentTitleList'>
          <div [ngStyle]='item.style'>{{item.text}}</div>
        </ng-template>
        <div class="stickborder"></div>
      </div>
      <ul [ngStyle]='item.style'>
        <ng-template ngFor let-i='index' let-item [ngForOf]='item.contentTextList'>
          <li>
            <div [twinkle]="item.twinkle" *ngIf='item.isShow' [ngStyle]='item.oneDivStyle' class="one"></div>
            <div class="two" [ngStyle]='item.style'>{{item.text}}</div>
          </li>
        </ng-template>
      </ul>
    </div>
  </ng-template>
  <ng-template ngFor let-i='index' let-item [ngForOf]='typetextlist[0].btnText'>
    <div class="btn" *ngIf='item.isShow'>
      <ul>
        <ng-template ngFor let-i='index' let-item [ngForOf]='item.btnTextList'>
          <li (click)='rotate($event,item.direction)' [ngStyle]='item.style'>{{item.text}}</li>
        </ng-template>
      </ul>
    </div>
  </ng-template>
  <ng-template ngFor let-i='index' let-item [ngForOf]='typetextlist[0].anatomyText'>
    <div class="anatomy" *ngIf='item.isShow'>
      <ul>
        <ng-template ngFor let-i='index' let-item [ngForOf]='item.anatomyTextList'>
          <li (click)='anatomyShow(item,i)' [ngStyle]='item.style'>
            {{item.text}}
          </li>
        </ng-template>
      </ul>
    </div>
  </ng-template>
  <div *ngIf='this.appdataService.content[this.current].rotateImgIsShow' class="tip">
    <ng-template ngFor let-i='index' let-item [ngForOf]='typetextlist[0].imglist'>
      <div *ngIf='item.isShow'>
        <!-- <img [ngStyle]='item.style' src="{{item.backgroundList[0].src}}" alt=""> -->
        <img class="changjing" [ngStyle]='item.style' src="{{item.contentList[0].src}}" alt="">
        <img class="changjing1" src="./assets/images/project/changjing1.png" alt="">
        <img class="changjing2" src="./assets/images/project/changjing2.png" alt="">
        <ul>
          <ng-template ngFor let-i='index' let-item [ngForOf]='item.contentList'>
            <span *ngIf='item.isShow'>
              <ng-template ngFor let-i='index' let-item [ngForOf]='item.textlist'>
                <li (click)='tipShow(item,i,false)' [twinkle]="item.twinkle" [ngStyle]='item.style'></li>
                <div *ngIf='item.isShow' [ngStyle]='item.content.style'>{{item.content.text}}</div>
              </ng-template>
            </span>
          </ng-template>
        </ul>
      </div>
    </ng-template>
  </div>
  <div class="childPage" *ngIf='this.appdataService.content[this.current].anatomyIsShow'>
    <img src="{{typetextlist[0].tiplist.src}}" alt="">
    <ul>
      <ng-template ngFor let-i='index' let-childitem [ngForOf]='typetextlist[0].tiplist.textlist'>
        <div class="childtitle" *ngIf='childitem.isShow'>{{childitem.title}}</div>
        <li class="anatomyDot" (click)='tipShow(childitem,i,false)' [ngStyle]='childitem.style'></li>
        <div class="contain" *ngIf='childitem.isShow'>
          <div (click)='childCloseTip(childitem)' class="childClose"></div>
          <div style="top:110px" class="nav">
            <ng-template ngFor let-i='index' let-item [ngForOf]='childitem.childPage.navText'>
              <span (click)='childRotateNav(childitem,i)' [ngStyle]='item.style'>{{item.text}}</span>
            </ng-template>
          </div>
          <ng-template ngFor let-='index' let-item [ngForOf]='childitem.childPage.content'>
            <div class="text" *ngIf='item.isShow'>
              <ul [ngStyle]='item.style'>
                <ng-template ngFor let-i='index' let-item [ngForOf]='item.contentTextList'>
                  <li>
                    <div *ngIf='item.isShow' [ngStyle]='item.oneDivStyle'></div>
                    <div [ngStyle]='item.style'>{{item.text}}</div>
                  </li>
                </ng-template>
              </ul>
            </div>
          </ng-template>
          <ng-template ngFor let-i='index' let-item [ngForOf]='childitem.childPage.btnText'>
            <div class="childBtn" *ngIf='item.isShow'>
              <ul>
                <ng-template ngFor let-i='index' let-item [ngForOf]='item.btnTextList'>
                  <li (click)='childRotate($event,item,childitem)' [ngStyle]='item.style'>{{item.text}}</li>
                </ng-template>
              </ul>
            </div>
          </ng-template>
          <div class="tip">
            <ng-template ngFor let-i='index' let-item [ngForOf]='childitem.childPage.imglist'>
              <div *ngIf='item.isShow'>
                <img src="{{item.contentList[0].src}}" alt=""> {{item.contentList.textlist}}
                <ul>
                  <ng-template ngFor let-i='index' let-item [ngForOf]='item.contentList'>
                    <span *ngIf='item.isShow'>
                      <ng-template ngFor let-i='index' let-item [ngForOf]='item.textlist'>
                        <li [twinkle]="item.twinkle" (click)='tipShow(item,i,false)' [ngStyle]='item.style'></li>
                        <div *ngIf='item.isShow' [ngStyle]='item.content.style'>{{item.content.text}}</div>
                      </ng-template>
                    </span>
                  </ng-template>
                </ul>
              </div>
            </ng-template>
          </div>
        </div>
      </ng-template>
    </ul>
  </div>
  <ng-template ngFor let-i='index' let-item [ngForOf]='typetextlist[0].measureList'>
    <div *ngIf='item.isShow'>
      <test-questions [chapter]='item.chapter'></test-questions>
    </div>
  </ng-template>
</div>

